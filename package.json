    "@react-native/normalize-colors": "^0.82.1",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@shopify/react-native-skia": "2.2.0",
    "fuse.js": "^7.1.0",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-fast-tflite": "^1.6.1",
